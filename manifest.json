{
  "name": "seller-addons",
  "vendor": "ssesandbox04",
  "version": "0.1.0",
  "title": "Seller Addons",
  "description": "Provide custom addons for sellers.",
  "scripts": {
    "postreleasy": "vtex publish --force"
  },
  "dependencies": {
    "vtex.store": "2.x",
    "vtex.rich-text": "0.x",
    "vtex.flex-layout": "0.x",
    "vtex.product-summary": "2.x",
    "vtex.store-components": "3.x",
    "vtex.order-manager": "0.x",
    "vtex.search-result": "3.x",
    "vtex.breadcrumb": "1.x",
    "vtex.responsive-layout": "0.x",
    "vtex.store-icons": "0.x",
    "vtex.styleguide": "9.x",
    "vtex.css-handles": "1.x",
    "vtex.session-client": "1.x",
    "vtex.file-manager-graphql": "0.x"
  },
  "builders": {
    "node": "6.x",
    "docs": "0.x",
    "react": "3.x",
    "masterdata": "1.x",
    "graphql": "1.x",
    "store": "0.x",
    "messages": "1.x",
    "admin": "0.x"
  },
  "credentialType": "absolute",
  "policies": [
    {
      "name": "ADMIN_DS"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.vtex.com",
        "path": "/api/dataentities/*"
      }
    },
    {
      "name": "AcessaTodosPedidos"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/api/checkout/pvt/*"
      }
    },
    {
      "name": "SaveOrderFormConfiguration"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "portal.vtexcommercestable.com.br",
        "path": "/api/checkout/pvt/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "httpstat.us",
        "path": "*"
      }
    },
    {
      "name": "colossus-fire-event"
    },
    {
      "name": "colossus-write-logs"
    }
  ],
  "billingOptions": {
    "type": "free",
    "availableCountries": [
      "*"
    ],
    "support": {
      "url": "https://support.vtex.com/hc/requests"
    }
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
